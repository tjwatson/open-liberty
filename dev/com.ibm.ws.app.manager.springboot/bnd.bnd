-include= ~../cnf/resources/bnd/bundle.props
-nouses=true
bVersion=1.0

Bundle-Name: WebSphere Spring Boot Support
Bundle-SymbolicName: com.ibm.ws.app.manager.springboot
Bundle-Description: WebSphere Spring Boot Support, version ${bVersion}

Private-Package: com.ibm.ws.app.manager.springboot.*

Include-Resource: OSGI-INF=resources/OSGI-INF
    
-dsannotations-inherit: true
-dsannotations: com.ibm.ws.app.manager.springboot.internal.SpringBootHandler

instrument.classesExcludes: com/ibm/ws/app/manager/springboot/internal/resources/*.class

-buildpath: \
	com.ibm.ws.adaptable.module;version=latest,\
	com.ibm.ws.app.manager;version=latest,\
	com.ibm.ws.app.manager.module;version=latest,\
	com.ibm.ws.artifact;version=latest,\
	com.ibm.ws.kernel.service,\
	com.ibm.ws.logging.core,\
	com.ibm.websphere.org.osgi.core,\
	com.ibm.websphere.org.osgi.service.component,\
	com.ibm.wsspi.org.osgi.service.component.annotations,\
	com.ibm.ws.container.service;version=latest,\
	com.ibm.ws.classloading;version=latest

-testpath: \
	../build.sharedResources/lib/junit/old/junit.jar;version=file, \
	com.ibm.ws.junit.extensions;version=latest, \
	org.hamcrest:hamcrest-all;version=1.3, \
	org.jmock:jmock-junit4;strategy=exact;version=2.5.1, \
	org.jmock:jmock;strategy=exact;version=2.5.1
